@{
    ViewData["Title"] = "nhap";
}
<h2>Nhap thong tin sinh vien</h2>
<form method="post"asp-action="Dssv">
    <label>Ma SV: </label>
    <input type="text" name="id" required /><br/>
    <label>Ten SV: </label>
    <input type="text" name="name" required /><br/>
    <label>Tuoi: </label>
    <input type="number" name="age" min="18" max="26" required /><br/>
    <label>GPA: </label>
    <input type="number" step="0.01" manifest="0" max="4" name="gpa" required /><br/>
    <button type=""submit">Gui</button>
</form>